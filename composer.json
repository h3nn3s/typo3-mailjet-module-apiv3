{
    "name": "mailjet",
    "type": "typo3-cms-extension",
    "version": "2.0.0",
    "description": "Mailjet integration to let users subscribe to a specific list.",
    "keywords": ["TYPO3", "extension", "mailjet"],
    "homepage": "https://www.mailjet.com",
    "authors": [
        {
            "name": "Alex Jelin",
            "role": "Developer",
            "homepage": "https://www.mailjet.com"
        }
    ],
    "license": "GPL-2.0-or-later",
    "require": {
        "typo3/cms-core": "^10.4 || ^11.0"
    },
    "suggest": {
        "helhum/typoscript-rendering": "^2.4"
    },
    "require-dev": {
        "php": ">=7.2.0",
        "phpunit/phpunit": "~5.3.0"
    },
    "autoload": {
        "psr-4": {
            "Api\\Mailjet\\": "Classes"
        }
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "scripts": {
        "post-autoload-dump": [
            "mkdir -p .Build/Web/typo3conf/ext/",
            "[ -L .Build/Web/typo3conf/ext/mailjet ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/mailjet"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0"
        },
        "typo3/cms": {
            "web-dir": ".Build/Web",
            "extension-key": "mailjet"
        }
    }
}
